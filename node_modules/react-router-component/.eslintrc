{
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "rules": {
    "strict": 0,
    "quotes": ["single"],
    "curly": "multi-line",
    "no-use-before-define": "nofunc",
    "no-underscore-dangle": 0,
    "no-unused-vars": [2, {"vars": "local", "args": "none"}],
    "new-cap": 0,
    "consistent-return": 0,
    "camelcase": 0,
    "dot-notation": 0,
    "no-multi-spaces": 0
  },
  env: {
    "browser": true,
    "node": true,
    "mocha": true
  }
}
